<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="RentospectMobileApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:RentospectMobileApp.Views"
    FlyoutBehavior="Disabled"
    Title="RentospectMobileApp">

    <ShellContent
        ContentTemplate="{DataTemplate local:LoginView}"
        Route="LoginView" />
    <ShellContent
        ContentTemplate="{DataTemplate local:PendingInspectionsView}"
        Route="PendingInspectionsView" />
</Shell>
